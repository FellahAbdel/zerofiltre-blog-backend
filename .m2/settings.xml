<settings>
    <pluginGroups>
        <pluginGroup>org.sonarsource.scanner.maven</pluginGroup>
    </pluginGroups>
    <profiles>
        <profile>
            <id>sonar</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <sonar.host.url>
                    http://sonarqube.zerofiltre.tech
                </sonar.host.url>
                <sonar.exclusions>
                    /tech/zerofiltre/**/model/**/*, /tech/zerofiltre/**/*JPA.*, **/config/*, **/util/*, **/*Exception.*
                </sonar.exclusions>
                <sonar.projectKey>tech.zerofiltre:blog</sonar.projectKey>
            </properties>
        </profile>
    </profiles>
</settings>


